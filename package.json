{
  "name": "singular-web-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "build": "browserify -t [ babelify --presets [ @babel/preset-env ] --global true ] public/javascripts/adobe-alloy.js -o public/javascripts/bundle.js",
    "watch": "watchify -t [ babelify --presets [ @babel/preset-env ] --global true ] public/javascripts/adobe-alloy.js -o public/javascripts/bundle.js -v"
  },
  "dependencies": {
    "@adobe/alloy": "^2.6.4",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "crypto-js": "4.2.0",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-useragent": "^1.0.15",
    "frontend-dependencies": "^2.0.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11",
    "request-ip": "^2.1.3",
    "snowflake-sdk": "^1.6.10"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ],
          "global": true,
          "ignore": [
            "/node_modules/(?!@adobe|uuid)/"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "7.26.9",
    "@babel/preset-env": "7.26.9",
    "babelify": "10.0.0",
    "browserify": "17.0.1",
    "watchify": "4.0.0",
    "nodemon": "3.0.1"
  }
}
